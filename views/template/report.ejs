<%layout('../layout')%>
<!-- Page Content -->
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Report</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
  		<div class="row">
  			<div class="col-lg-6">
          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-bordered table-hover table-striped">
                <thead>
                  <tr>
                    <th>공격자 IP</th>
                    <th>목적지 IP</th>
                    <th>공격 일시</th>
                    <th>시도 횟수</th>
                  </tr>
                </thead>
                <tbody>
              <% for(var i in info){%>
                  <tr>
                    <td><%=info[i].source_ip%></td>
                    <td><%=info[i].destination_ip%></td>
                    <td><%=info[i].time%></td>
                    <td><%=info[i].number%></td>
                  </tr>
              <%}%>
                </tbody>
              </table>
            </div>
  			</div>
  		</div>
    </div>
    <!-- /.container-fluid -->
</div>
<!-- /#page-wrapper -->
