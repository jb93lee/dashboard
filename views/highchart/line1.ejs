<style>
#container1 {
    height: 430px;
    width: 725px;
    margin: 0 auto;
}

.highcharts-tooltip>span {
    padding: 10px;
    white-space: normal !important;
    width: 200px;
}

.loading {
    margin-top: 10em;
    text-align: center;
    color: gray;
}

.f32 .flag {
    vertical-align: middle !important;
}
</style>
<script src="highchart/highstock.js"></script>
<script src="highchart/exporting.js"></script>


<div id="container1" style="height: 400px; min-width: 310px"></div>
<script>
document.addEventListener("DOMContentLoaded", function(event) {
       var seriesOptions = [],
           seriesCounter = 0,
           names = ['ips','waf'];

       /**
        * Create the chart when all data is loaded
        * @returns {undefined}
        */
       function createChart() {

           $('#container1').highcharts('StockChart', {

               rangeSelector: {
                   selected: 4
               },

               yAxis: {
                   labels: {
                       formatter: function () {
                           return (this.value > 0 ? ' + ' : '') + this.value + '%';
                       }
                   },
                   plotLines: [{
                       value: 0,
                       width: 2,
                       color: 'silver'
                   }]
               },

               plotOptions: {
                   series: {
                       compare: 'percent'
                   }
               },

               tooltip: {
                   pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.y}</b> ({point.change}%)<br/>',
                   valueDecimals: 0
               },

               series: seriesOptions
           });
       }

       $.each(names, function (i, name) {

//           $.getJSON('https://www.highcharts.com/samples/data/jsonp.php?filename=' + name.toLowerCase() + '-c.json&callback=?',    function (data) {
//            $.getJSON('http://192.168.33.26/jsonp.php?filename=' + name.toLowerCase() + '.json&callback=?', function (data) {
          $.getJSON('http://192.168.0.110/project/jsonp.php?filename=' + name.toLowerCase() + '.json&callback=?', function (data) {
              seriesOptions[i] = {
                   name: name,
                   data: data
               };

               // As we're loading the data asynchronously, we don't know what order it will arrive. So
               // we keep a counter and create the chart when all the data is loaded.
               seriesCounter += 1;

               if (seriesCounter === names.length) {
                   createChart();
               }
           });
       });
});
</script>
<script type="text/javascript" src="highchart/dark-unica.js"></script>
